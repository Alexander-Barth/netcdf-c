# Test c output
T=tst_compress
VG=valgrind --leak-check=full
GDB=gdb --args
#ARGS="nozip zip bzip2"

CFLAGS=-O0 -gstabs+ -I.. -I../include -I/usr/local/include
LDFLAGS=../liblib/.libs/libnetcdf.a  -L/usr/local/lib -lhdf5_hl -lhdf5 -lz -lm -lcurl -lbz2

EXP=export LD_LIBRARY_PATH="/usr/local/lib"

#	cd .. ; ${MAKE} all

all::
	${EXP}; gcc -o t ${CFLAGS} ${T}.c ${LDFLAGS}

t::
	${EXP}; ./t ${ARGS}

v::
	${EXPR}; ${VG} ./t ${ARGS}

g::
	${EXP}; ${GDB} ./t ${ARGS}
